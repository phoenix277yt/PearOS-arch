pkgname=pear-resources
pkgver=1.0
pkgrel=1
epoch=
pkgdesc="Icons and scripts for Pear KDE and updater"
arch=('any')
url="https://github.com/phoenix277yt/PearOS-arch"
license=('GPL')
source=("resources.tar.gz")
md5sums=('074c1e69cc7514a6ec7c07505dcee049')

package() {
	mkdir -p $pkgdir/usr/share/
	tar -xvf resources.tar.gz -C $pkgdir/usr/share/.
}
