pkgname=pear-skel
pkgver=2.0
pkgrel=1
epoch=
pkgdesc="Icons and scripts for Pear KDE and updater"
arch=('any')
url="https://github.com/phoenix277yt/PearOS-arch"
license=('GPL')
source=("resources.tar.gz")
md5sums=('f2cc4984fc50d84ce5ecf14eb8868b5d')

package() {
	mkdir -p $pkgdir/etc/skel
	tar -xf resources.tar.gz -C $pkgdir/etc/skel/.
}
