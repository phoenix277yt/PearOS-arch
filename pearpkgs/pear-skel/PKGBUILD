# Maintainer: Matt C <matt@xhec.us>
pkgname=pear-skel
pkgver=2.1
pkgrel=1
epoch=
pkgdesc="Icons and scripts for Pear KDE and updater"
arch=('any')
url="https://github.com/phoenix277yt/PearOS-arch"
license=('GPL')
source=("skel.tar.gz")
noextract=("skel.tar.gz")
md5sums=('aa67458ca0ed27e4323794db3f3628f5')

package() {
	mkdir -p $pkgdir/etc/skel
	tar -xf skel.tar.gz -C $pkgdir/etc/skel/.
	rm skel.tar.gz
}
